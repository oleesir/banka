{
  "name": "banka",
  "version": "1.0.0",
  "description": "A banking application that helps users perform banking transactions.",
  "main": "index.js",
  "scripts": {
    "build": "babel server/src -d server/dist",
    "start": "node ./server/dist/app.js",
    "dev": "nodemon ./server/src/app.js --exec babel-node",
    "pretest": "npm run db:migrate:test",
    "test": "NODE_ENV=test mocha --require @babel/register server/test/tests.test.js --exit",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "test:coverage": "nyc --reporter=text --reporter=json --reporter=lcov npm run test",
    "codecov": "codecov",
    "lint": "eslint \"**/*.js\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oleesir/banka.git"
  },
  "author": "Olisa Emodi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oleesir/banka/issues"
  },
  "homepage": "https://github.com/oleesir/banka#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.11.0",
    "sequelize-cli": "^5.5.0",
    "uuid": "^3.3.2"
  }
}
