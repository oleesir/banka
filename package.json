{
  "name": "banka",
  "version": "1.0.0",
  "description": "A banking application that helps users perform banking transactions.",
  "main": "index.js",
  "scripts": {
    "build": "babel server/src -d server/dist",
    "start": "node ./server/dist/app.js",
    "dev": "nodemon ./server/src/app.js --exec babel-node",
    "test": "mocha --require @babel/register server/test/tests.test.js --exit",
    "test:coverage": "nyc --reporter=text --reporter=json --reporter=lcov mocha --require @babel/register server/test/tests.test.js --exit",
    "codecov": "codecov",
    "lint": "eslint \"**/*.js\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oleesir/banka.git"
  },
  "author": "Olisa Emodi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oleesir/banka/issues"
  },
  "homepage": "https://github.com/oleesir/banka#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
